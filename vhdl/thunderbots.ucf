# Global configuration options
config part=xc6slx9-tqg144-2;
config vccaux=3.3;
config post_crc=enable;

# 8MHz oscillator
timespec "TSOscillator" = PERIOD "Oscillator" 125 ns INPUT_JITTER 6.875 ps; # Input jitter computed from oscillator datasheet as frequency stability (50 ppm) plus aging (5 ppm)
net "OscillatorPin<0>" iostandard=LVCMOS33 | float | tnm_net="Oscillator" | loc=P88;
net "OscillatorPin<1>" iostandard=LVCMOS33 | float | tnm_net="Oscillator" | loc=P92;
net "OscillatorEnablePin" iostandard=LVCMOS33 | slew=quietio | loc=P93;

# Power control
net "LogicPowerPin" iostandard=LVCMOS33 | slew=quietio | loc=P137;
net "HVPowerPin" iostandard=LVCMOS33 | slew=quietio | loc=P138;

# SPI ADC
net "ADCCSPin" iostandard=LVCMOS33 | slew=quietio | loc=P35;
net "ADCClockPin" iostandard=LVCMOS33 | slew=quietio | loc=P87;
net "ADCMOSIPin" iostandard=LVCMOS33 | slew=quietio | loc=P34;
net "ADCMISOPin" iostandard=LVCMOS33 | float | loc=P33;

# SPI flash and debug port
